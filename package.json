{
	"name": "rethink_nodejs_clusterizing",
	"version": "1.0.0",
	"description": "Exemplificando a modulo cluster do Node.js junto ao pm2",
	"main": "app.js",
	"scripts": {
		"start": "node cluster.js",
		"start:process": "PORT=3000 node app.js",
		"start:cluster": "PORT=3001 node cluster.js",
