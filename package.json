		"pm2:monit": "pm2 monit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Bergsonjr/rethink_nodejs_clusterizing.git"
	},
	"author": "Bergson Jr.",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Bergsonjr/rethink_nodejs_clusterizing/issues"
	},
	"homepage": "https://github.com/Bergsonjr/rethink_nodejs_clusterizing#readme",
	"dependencies": {
		"fastify": "^3.27.0",
		"pm2": "^5.1.2"
	},
	"devDependencies": {
		"autocannon": "^7.6.0",
		"eslint": "^8.8.0",
		"prettier": "^2.5.1"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"commonjs": true,
			"es2021": true
